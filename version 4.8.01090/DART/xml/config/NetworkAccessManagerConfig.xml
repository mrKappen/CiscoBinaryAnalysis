          </action>
          <action>
            <path>%WINDIR%\system32\wbem</path>
            <args>wmic.exe nteventlog where " logfilename='Cisco AnyConnect Network Access Manager' " call backupeventlog "%TEMP%\NetworkAccessManager.evt"</args>
