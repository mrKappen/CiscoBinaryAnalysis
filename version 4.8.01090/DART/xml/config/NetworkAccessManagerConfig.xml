  <file id="nam-logs">
    <default>true</default>
    <description>NAM application logs</description>
    <gui>
      <label>_tr("NAM application logs")</label>
      <file_type>log</file_type>
    </gui>
    <dart>
      <required>false</required>
	  <os opsys="win8">
        <file_copy_action>
          <action>
            <loc>%ProgramData%\Cisco\Cisco AnyConnect Secure Mobility Client\Network Access Manager\logs\*.cap</loc>
            <clear_log apply="true"></clear_log>
          </action>
        </file_copy_action>
        <use_extern_action>
          <action>
            <plugin></plugin>
            <args>wineventlog.exe -d -l "Cisco AnyConnect Network Access Manager" -f %TEMP%\NetworkAccessManager.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32</path>
            <args>wevtutil epl "Cisco AnyConnect Network Access Manager" %TEMP%\NetworkAccessManager.evtx</args>
            <clear_log apply="true">wevtutil cl "Cisco AnyConnect Network Access Manager"</clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.evtx</temp_out>
          </action>
          <action>
            <plugin></plugin>
            <args>acnampkgplugin.exe -f %TEMP%\NetworkAccessManagerPlugin.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManagerPlugin.txt</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="win7">
        <file_copy_action>
          <action>
            <loc>%ProgramData%\Cisco\Cisco AnyConnect Secure Mobility Client\Network Access Manager\logs\*.cap</loc>
            <clear_log apply="true"></clear_log>
          </action>
        </file_copy_action>
        <use_extern_action>
          <action>
            <plugin></plugin>
            <args>wineventlog.exe -d -l "Cisco AnyConnect Network Access Manager" -f %TEMP%\NetworkAccessManager.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32</path>
            <args>wevtutil epl "Cisco AnyConnect Network Access Manager" %TEMP%\NetworkAccessManager.evtx</args>
            <clear_log apply="true">wevtutil cl "Cisco AnyConnect Network Access Manager"</clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.evtx</temp_out>
          </action>
          <action>
            <plugin></plugin>
            <args>acnampkgplugin.exe -f %TEMP%\NetworkAccessManagerPlugin.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManagerPlugin.txt</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="vista">
        <file_copy_action>
          <action>
            <loc>%ProgramData%\Cisco\Cisco AnyConnect Secure Mobility Client\Network Access Manager\logs\*.cap</loc>
            <clear_log apply="false"></clear_log>
          </action>
        </file_copy_action>
        <use_extern_action>
          <action>
            <plugin></plugin>
            <args>wineventlog.exe -d -l "Cisco AnyConnect Network Access Manager" -f %TEMP%\NetworkAccessManager.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32</path>
            <args>wevtutil epl "Cisco AnyConnect Network Access Manager" %TEMP%\NetworkAccessManager.evtx</args>
            <clear_log apply="true">wevtutil cl "Cisco AnyConnect Network Access Manager"</clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.evtx</temp_out>
          </action>
          <action>
            <plugin></plugin>
            <args>acnampkgplugin.exe -f %TEMP%\NetworkAccessManagerPlugin.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManagerPlugin.txt</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="winxp">
        <file_copy_action>
          <action>
            <loc>%ALLUSERSPROFILE%\Application Data\Cisco\Cisco AnyConnect Secure Mobility Client\Network Access Manager\logs\*.cap</loc>
            <clear_log apply="false"></clear_log>
          </action>
        </file_copy_action>
        <use_extern_action>
          <action>
            <plugin></plugin>
            <args>wineventlog.exe -d -l "Cisco AnyConnect Network Access Manager" -f %TEMP%\NetworkAccessManager.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32\wbem</path>
            <args>wmic.exe nteventlog where " logfilename='Cisco AnyConnect Network Access Manager' " call backupeventlog "%TEMP%\NetworkAccessManager.evt"</args>
            <clear_log apply="true">wmic.exe nteventlog where " logfilename='Cisco AnyConnect Network Access Manager' " call ClearEventLog</clear_log>
            <temp_out>%TEMP%\NetworkAccessManager.evt</temp_out>
          </action>
          <action>
            <plugin></plugin>
            <args>acnampkgplugin.exe -f %TEMP%\NetworkAccessManagerPlugin.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\NetworkAccessManagerPlugin.txt</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="win2k3">
